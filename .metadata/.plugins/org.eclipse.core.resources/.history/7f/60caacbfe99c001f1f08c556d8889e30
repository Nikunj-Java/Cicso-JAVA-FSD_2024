package com.simplilearn.demo;

class User{
	String name;
	String phone;
	String email;
	User(){
		
	}
	
	 

	public User(String name, String phone, String email) {
		this.name = name;
		this.phone = phone;
		this.email = email;
	}



	@Override
	public String toString() {
		// TODO Auto-generated method stub
		return "User [name=" +name+",phone="+phone+",email="+email+"]";
	}
	
}


public class OptionalClass {
	
	public static void main(String[] args) {
		User u1= new User();
		User u2= new User("Nikunj Soni", "9988776655", "nikunj@cisco.com");
		User u3=null;
		
		//System.out.println("Name in u1 is: "+u1.name);
		System.out.println("Name in u2 is: "+u2.name);
		//System.out.println("Name in u1 is: "+u3.name);
	}

}
